    "let number =100 \n",
     "number += 1 \n",
     "\n",
    "// Operation: -= \n",
    "let number =100 \n",
     "number -= 1 \n",
     "\n",
     "// Operation: *= \n",
    "let number =100 \n",
     "number *= 1 \n",
     "\n",
     "// Operation: /= \n",
    "let number =100 \n",
     "number /= 1 \n",
     "\n",
     
